{% extends '::base.html.twig' %}

{% block title %}Add a exam{% endblock %}

{% block body %}
<div class="width-300 center-block">
  <h1 class="text-center text-primary">Add a exam</h1>
  {{ form_start(form) }}
    {{ form_rest(form) }}
    <input type="submit" class="btn btn-primary btn-block margin-top-5" value="Save"/>

    {{ form_end(form) }}
    <a class="btn btn-primary btn-block margin-top-5" href="{{ path('exam_list') }}">Exam list</a>

</div>
{% endblock %}
